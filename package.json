{
  "name": "electron-tunnels",
  "version": "1.1.0",
  "description": "Electron app to manage your ssh tunnels",
  "repository": "parro-it/tunnels",
  "author": "andrea@parro.it",
  "keywords": [
    "electron",
    "ssh",
    "tunnel",
    "port",
    "forwarding"
  ],
  "dependencies": {
    "co": "^4.6.0",
    "dom-delegate": "^2.0.3",
    "electron-debug": "^0.5.1",
    "electron-detach": "^0.3.0",
    "electron-window": "^0.6.0",
    "insert-css": "^0.2.0",
    "menubar": "^2.3.0",
    "node-uuid": "^1.4.3",
    "nunjucks": "^2.0.0",
    "open-ssh-tunnel": "^0.1.0",
    "photon": "github:connors/photon",
    "store": "^1.3.17"
  },
  "scripts": {
    "test": "node test | faucet",
    "rebuild": "electron-rebuild",
    "lint": "eslint .",
    "start": "electron .",
    "package:osx":  "electron-packager . Tunnels --platform=darwin --arch=x64 --version=0.35.2 --ignore='(media|test|private|node_modules/electron-packager|node_modules/electron-prebuilt)' --overwrite --out=setup",
    "package:linux:64":  "electron-packager . Tunnels --platform=linux  --arch=x64 --version=0.35.2 --ignore='(media|test|private|node_modules/electron-packager|node_modules/electron-prebuilt)' --overwrite --out=setup",
    "package:linux:32":  "electron-packager . Tunnels --platform=linux --arch=ia32 --version=0.35.2 --ignore='(media|test|private|node_modules/electron-packager|node_modules/electron-prebuilt)' --overwrite --out=setup"

  },
  "files": [
    "index.js",
    "src"
  ],
  "bin": {
    "tunnels": "cli.js"
  },
  "eslintConfig": {
    "extends": "js"
  },
  "license": "MIT",
  "devDependencies": {
    "electron-packager": "^5.1.1",
    "electron-prebuilt": "^0.35.2",
    "tape": "^4.2.2"
  }
}
