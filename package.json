{
  "name": "electron-tunnels",
  "version": "1.2.0",
  "description": "Electron app to manage your ssh tunnels",
  "repository": "parro-it/tunnels",
  "author": "andrea@parro.it",
  "scripts": {
    "rebuild": "electron-rebuild",
    "test": "xo",
    "start": "ELECTRON_ENV=development electron .",
    "package:build": "scripty",
    "package:darwin": "platform=darwin arch=x64 npm run package:build",
    "package:linux:x64": "platform=linux arch=x64 npm run package:build",
    "package:linux:ia32": "platform=linux arch=ia32 npm run package:build",
    "package:win32:x64": "platform=win32 arch=x64 npm run package:build",
    "package:win32:ia32": "platform=win32 arch=ia32 npm run package:build",
    "package": "scripty"
  },
  "main": "index.js",
  "files": [
    "src",
    "index.js",
    "cli.js"
  ],
  "bin": {
    "tunnels": "cli.js"
  },
  "electronVersion": "0.37.8",
  "productName": "Tunnels",
  "license": "MIT",
  "babel": {
    "presets": [
      "node5",
      "react"
    ]
  },
  "xo": {
    "extends": "xo-react",
    "envs": [
      "browser",
      "node"
    ]
  },
  "keywords": [
    "electron",
    "ssh",
    "tunnel",
    "port",
    "forwarding"
  ],
  "dependencies": {
    "debug": "^2.2.0",
    "debug-menu": "^0.4.0",
    "electron-contextmenu-middleware": "^1.0.1",
    "electron-debug": "^0.7.0",
    "electron-input-menu": "^1.3.0",
    "electron-rebuild": "^1.1.3",
    "electron-window": "^0.7.0",
    "font-awesome": "^4.5.0",
    "is-electron-renderer": "^2.0.0",
    "is-reachable": "^1.3.0",
    "keytar": "^3.0.2",
    "map-obj": "^1.0.1",
    "node-uuid": "^1.4.3",
    "object-values": "^1.0.0",
    "open-ssh-tunnel": "^0.3.0",
    "photon": "github:connors/photon",
    "pify": "^2.3.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "redux": "^3.4.0",
    "redux-form": "^5.2.3",
    "redux-submitform-onchange": "^0.2.0",
    "redux-thunk": "^2.0.1",
    "store": "^1.3.17",
    "username": "^2.0.0",
    "validate-ip-node": "^1.0.6"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.8.0",
    "babel-preset-node5": "^11.0.2",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.5.2",
    "electron-packager": "^7.0.1",
    "electron-prebuilt": "0.37.8",
    "eslint-config-xo-react": "^0.7.0",
    "eslint-plugin-react": "^5.0.1",
    "scripty": "^1.5.0",
    "xo": "^0.15.0"
  }
}
